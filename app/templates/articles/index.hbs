<div class="page-header">
    <h3>Articles</h3>
</div>
<div class="list-group">
  {{#each model as |article|}}
    {{#link-to "articles.show" article class="list-group-item"}}
        <h4 class="list-group-item-heading">
          {{article.title}}
        </h4>
        <div class="list-group-item-text">Modified: {{article.dateModified}}</div>
    {{/link-to}}
  {{/each}}
</div>
